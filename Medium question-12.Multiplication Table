import java.util.Scanner;
public class multable 
{
	public static void main(String[] args) 
	{
		Scanner scanner = new Scanner(System.in);
		System.out.print("M = ");
		int m = scanner.nextInt();
		System.out.println();
		System.out.print("N = ");
		int n = scanner.nextInt();
		System.out.println();
		for(int i=1;i<=n;i++)
		{
			System.out.println(i+"x"+m+"="+(i*m));
			System.out.println();
		}
	}
}
