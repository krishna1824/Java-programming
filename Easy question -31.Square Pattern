import java.util.Scanner;
public class squarepattern 
{
	public static void main(String[] args) 
	{
        Scanner scanner = new Scanner(System.in);
		System.out.print("enter number:");
        int n = scanner.nextInt();
        int k = 1;
        for(int j=1;j<=n;j++)
        {
          for(int i=1;i<=j;i++)
           {

              System.out.print(k*k+" ");
              k++;	

           }
          System.out.println();
        }

	}
}
