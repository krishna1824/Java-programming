import java.util.Scanner;
public class patterntriangle
{
	public static void main(String[] args) 
	{
        Scanner scanner = new Scanner(System.in);
		System.out.print("Enter the number:");
        int n = scanner.nextInt();
        for (int j=1;j<=n;j++)
        {
           for (int i=1;i<=j;i++)
           {
             System.out.print(j);
           }
           System.out.println();     
        }
	}
}
