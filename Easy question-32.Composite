import java.util.Scanner;
public class odd 
{
	public static void main(String[] args) 
	{
        Scanner scanner = new Scanner(System.in);
		System.out.print(" N = ");
        int n = scanner.nextInt();
        int arr[]=new int[100];
        int j=1;
        for(int i=1;i<100;i++)
         {
           if(i%2!=0) 
            {
               arr[j] = i;
               j++;
            }
         }
         System.out.print("4th Odd number after 4 odd numbers = " +arr[n*2]);
	}
}
